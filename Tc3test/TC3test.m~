[X,L,C]=normalData(X);
kmin=2;
kmax=10;
nr=20;
valueold=0;
 for i=kmin:kmax;
  for j=1:nr;
  [BestProt,BestSSD,Bestlabel,value,b]=kmedias(L,i,X);     
  MtxBestProt(:,:,j)=BestProt;
  MtxBestSSD(:,:,j)=BestSSD;
  MtxBestlabel(:,:,j)=BestSSD;

  end
end